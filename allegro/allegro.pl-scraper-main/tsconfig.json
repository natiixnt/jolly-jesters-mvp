{
    "compilerOptions": {
        "target": "ES2022",
        "module": "Preserve",
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "rootDir": "src",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": ["src"]
}
